{
    "name": "gestion-novedades",
    "version": "1.0.0",
    "description": "Proyecto de gestión de novedades para el departamento de criminalística del centro de investigaciones científicas, penales y criminalísticas (CICPC)",
    "main": "index.js",
    "scripts": {
        "prestart": "babel server --out-dir dist-server",
        "start": "node dist-server/index.js",
        "starting": "concurrently -r \"npm run start:server\" \"npm run start:client\" ",
        "start:server": "cross-env NODE_ENV=development nodemon server/index.js --exec babel-node",
        "start:client": "npm run serve src2/"
    },
    "private": true,
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "bcrypt": "^1.0.3",
        "body-parser": "^1.18.2",
        "chalk": "^2.3.0",
        "concurrently": "^3.5.1",
        "core-js": "^2.4.1",
        "cross-env": "^5.1.1",
        "express": "^4.16.2",
        "express-asyncify": "^1.0.0",
        "isemail": "^3.0.0",
        "jsonwebtoken": "^8.1.0",
        "jwt-simple": "^0.5.1",
        "moment": "^2.22.0",
        "mongoose": "^5.0.14",
        "mysql": "^2.15.0",
        "mysql2": "^1.5.3",
        "ng2-toastr": "^4.1.2",
        "ng2-validation": "4.2.0",
        "password-validator": "^4.0.0",
        "rand-token": "^0.4.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.3",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "codelyzer": "^4.0.1",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "nodemon": "^1.12.1",
        "protractor": "~5.1.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3"
